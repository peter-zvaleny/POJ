#include <iostream>

using namespace std;

int main(){
	int MAX_N = 4
	int a[MAX_N] = {1, 2, 3, 4];
	int n, k;
}

bool dfs(int i, int sum){
	if(i == n) return sum == k;
	if(dfs(i + 1, sum)) return true;
	if(dfs(i + 1, sum + a[i])) return true;

	return false;	
}

void solve(){
	if(dfs(0, 0)) return 
}
